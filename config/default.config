[global]

    max_events = 100
    timeout = 30000

[/global]


[server]

    listen = 8080
    server_name = localhost
    max_body_size = 1000

[error_page]
    403 = ./error/403.html
    404 = ./error/404.html
    405 = ./error/405.html
    413 = ./error/413.html
    500 = ./error/500.html
[/error_page]

[location]
    request_path = /
    root = ./www
    index = index.html
    autoindex = on
    methods = POST GET DELETE
[/location]

[location]
    request_path = /upload
    root = /data/www/upload
    index = index.html
    autoindex = on
    methods = POST GET
[/location]

[location]
    request_path = /cgi-bin
    root = ./www/cgi-bin
    index = index.html
    autoindex = on    
    methods = POST GET
[/location]

[location]
    request_path = ./old
    root = ./old
    index = index.html
    autoindex = on
    methods = POST GET
    redirect = ./www
[/location]

[/server]

