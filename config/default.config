[global]

    max_events = 200
    timeout = 20000
    upload_directory = ./www/upload

[/global]


[server]

    listen = 8080
    server_name = localhost example.com
    max_body_size = 10000

[error_page]
    400 = ./error/400.html  
    403 = ./error/403.html
    404 = ./error/404.html
    405 = ./error/405.html
    406 = ./error/406.html
    413 = ./error/413.html
    500 = ./error/500.html
    501 = ./error/501.html
    505 = ./error/505.html
[/error_page]

[location]
    request_path = /
    root = ./www
    index = index.html
    autoindex = on
    methods = GET DELETE POST
[/location]

[location]
    request_path = /hello
    root = ./www
    index = index.html
    autoindex = on
    methods = GET
[/location]

[location]
    request_path = /upload
    root = ./www/upload
    index = index.html
    autoindex = on
    methods = POST GET DELETE
[/location]

[location]
    request_path = /images
    root = ./www/images
    index = index.html
    autoindex = on
    methods = GET
[/location]

[location]
    request_path = /cgi-bin
    root = ./www/cgi-bin
    index = index.html
    autoindex = on    
    methods = POST GET
[/location]

[location]
    request_path = /old
    root = ./old
    index = index.html
    autoindex = on
    methods = GET
    redirect = http://localhost:8080
[/location]

[/server]

